{
    "devDependencies": {
        "@truffle/hdwallet-provider": "^1.4.0",
        "csv-writer": "^1.6.0",
        "dotenv": "^9.0.2",
        "dotenv-cli": "^4.0.0",
        "ganache-cli": "^6.12.2",
        "node-fetch": "^2.6.1",
        "prettier-plugin-solidity": "^1.0.0-beta.10",
        "truffle": "^5.3.6",
        "truffle-plugin-verify": "^0.5.8"
    },
    "scripts": {
        "deploy:rinkeby": "dotenv -- truffle deploy --network ethRinkeby",
        "deploy:bscTestnet": "dotenv -- truffle deploy --network bscTestnet",
        "start:rinkebyFork": "dotenv -- bash -c 'ganache-cli -f https://eth-rinkeby.alchemyapi.io/v2/${ALCHEMY_API_KEY_RINKEBY}'",
        "start:bscMainnetFork": "ganache-cli -f https://bsc-dataseed1.binance.org",
        "test": "dotenv truffle test",
        "verify:rinkeby": "dotenv -- truffle run verify RailrToken --network ethRinkeby",
        "verify:bscTestnet": "dotenv -- truffle run verify RailrToken --network bscTestnet"
    }
}
